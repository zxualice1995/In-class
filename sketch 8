function setup() {
  createCanvas(400,400);
  background(255);
}

function draw() {
  for(var i=0; i<20; i=i+10){
    fill(255);
    stroke(0);
    ellipseMode(CENTER);
    ellipse(mouseX, mouseY, 35,35);
     ellipse(mouseX-10,mouseY-13,5,5);
     ellipse(mouseX+10,mouseY-13,5,5);
     ellipse(mouseX,mouseY+10,5,5);
    
  }
  // for(var i=0; i<20; i=i+1){
  //   rect(32,40,15,10);
  //   fill(255);
  //   stroke(255);
  //   }
}
function mousePressed(){
  clear();
  
}
