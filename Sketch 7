function setup() {
  createCanvas(200, 200);
  background(255);
}
var numberOfCircle = 0;

function draw() {
  var a = random(255);
  var b = random(255);
  var c = random(255);
  stroke(a, b, c);
  fill(a, b, c);
  var d = random(25);
  var e = random(150);
  var f = random(150);
  ellipse(e, f, d, d);
  numberOfCircle = numberOfCircle + 1;
  if (numberOfCircle > 400) {
    noLoop(a);
  }
}
